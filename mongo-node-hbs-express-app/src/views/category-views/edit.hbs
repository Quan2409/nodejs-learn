<h1>Edit New Category</h1>
<form action="" method="post">
  <input
    type="text"
    name="name"
    placeholder="Category Name"
    value={{categoryValue.name}}
    required
  />
  <br />
  <input
    type="text"
    name="description"
    placeholder="Category Description"
    value={{categoryValue.description}}
    required
  />
  <br />
  <button>Edit</button>
</form>